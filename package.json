{
  "name": "gatool-api",
  "version": "4.0.0",
  "repository": "https://github.com/arthurlockman/gatool",
  "private": true,
  "author": "James Lockman, Arthur Rosa",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prebuild": "eslint .",
    "build": "tsc",
    "start": "tsx src/app.ts",
    "start:no-redis": "export DISABLE_REDIS=true && tsx src/app.ts",
    "start:env": "tsx --env-file .env src/app.ts",
    "watch": "tsx watch src/app.ts",
    "watch:no-redis": "export DISABLE_REDIS=true && tsx watch src/app.ts",
    "watch:env": "tsx watch --env-file .env src/app.ts",
    "write-version": "echo $(git rev-parse HEAD) >> version.txt",
    "deploy:production": "pm2 deploy ecosystem.config.cjs production --force",
    "sync-users": "tsx src/cron/updateUsers.ts",
    "update-high-scores": "tsx src/cron/updateHighScores.ts",
    "create-dev-containers": "tsx src/utils/createDevContainers.ts"
  },
  "dependencies": {
    "@azure/identity": "4.10.2",
    "@azure/keyvault-secrets": "4.10.0",
    "@azure/storage-blob": "^12.28.0",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "auth0": "^4.27.0",
    "cron": "^4.3.2",
    "debug": "~4.4.1",
    "express": "~5.1.0",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "express-unless": "^2.1.3",
    "got": "^14.4.7",
    "lodash": "^4.17.21",
    "newrelic": "^13.0.0",
    "node_extra_ca_certs_mozilla_bundle": "^1.0.7",
    "node-cache": "^5.1.2",
    "p-queue": "^8.1.0",
    "pino-http": "^10.5.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.15",
    "@types/mailchimp__mailchimp_marketing": "^3.0.21",
    "@types/newrelic": "^9.14.8",
    "@types/node": "^22.13.4",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
