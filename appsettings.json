{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Azure.Core": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.NewRelic.Logs",
      "NewRelic.LogEnrichers.Serilog"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "NewRelicLogs" }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId", "WithNewRelicLogsInContext"]
  },
  "AllowedHosts": "*",
  "KeyVaultUrl": "https://GAToolApiKeys.vault.azure.net",
  "Redis": {
    "Host": "localhost",
    "Port": "6379",
    "UseTls": false,
    "Password": ""
  }
}
