{
  "KeyVaultUrl": "https://GAToolApiKeys.vault.azure.net",
  "Redis": {
    "Host": "localhost",
    "Port": "6379",
    "UseTls": false,
    "Password": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  }
}
